    <!-- https://www.museum4punkt0.de/catalogue/ontology/neededExpertise -->

    <owl:DatatypeProperty rdf:about="https://www.museum4punkt0.de/catalogue/ontology/neededExpertise">
        <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E65_Creation"/>
